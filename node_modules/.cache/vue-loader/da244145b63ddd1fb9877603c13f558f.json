n\t\t\t} catch(error){\n\t\t\t\tconsole.log(error)\n\t\t\t}\n\t\t},\n    changeLoader(payload) {\n\t\t\tthis.loader = payload;\n\t\t},\n\t\tdeleteRoom(payload){\n      try { \n        axios\n          .delete(this.serverURL + `/rooms/delete/${payload.id}`)\n          .then(response => {\n              console.log('La habitacion ha sido eliminado.');\n              this.getRooms();              \n          })\n          .catch(e => {\n            this.getRooms();  \n            console.log(e.response);\n          })\n      } catch (error) {\n        this.getRooms();  \n        console.log(error)\n      }\n    }\n\t},\n\t\n  computed: {\n    ...mapState([\n      'serverURL',\n    ]),\n  }\t\n}\n</script>"]}]}